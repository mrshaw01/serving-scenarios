# Copy this file to `.env` and fill in real values.
# Never commit `.env` (it is ignored by `.gitignore`).

# OpenAI-compatible base URL (include /v1)
OPENAI_BASE_URL=https://<your-tensormesh-domain>/v1

# Bearer token used as: Authorization: Bearer <token>
OPENAI_API_KEY=<paste_your_token_here>

# Required by Tensormesh gateway for attribution/routing
TENSORMESH_USER_ID=<your_user_id_uuid>

# Deployed model id/name
OPENAI_MODEL=openai/gpt-oss-120b
